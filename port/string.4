: , 
	HERE @ !
	HERE @ WS + HERE !
;

: C,
	HERE @ C!
	HERE @ 1 + HERE !
;

: STRCMP
	:STRCMPLOOP
		2DUP
		C@ SWAP C@ = ZGOTO STRCMPNOTSAME
		2DUP
		C@ INV SWAP C@ INV AND NZGOTO STRCMPSAME
		2DUP
		C@ 0 = SWAP C@ 0 = OR NZGOTO STRCMPNOTSAME
		1 + SWAP 1 +
		GOTO STRCMPLOOP
	:STRCMPNOTSAME
		DROP DROP 0
		GOTO ENDSTRCMP
	:STRCMPSAME
		DROP DROP 1
		GOTO ENDSTRCMP
	:ENDSTRCMP
;

: STRLEN
	0 SWAP
	:STRLENLOOP
		DUP C@ ZGOTO ENDSTRLEN
		SWAP 1 + SWAP
		1 +
		GOTO STRLENLOOP
	:ENDSTRLEN
		DROP 
;

: STRSAVE
	DUP STRLEN
	SWAP TARGET !
	:STRSAVELOOP
		TARGET @ C@ C,
		TARGET @ 1 + TARGET !
		1 -
		DUP NZGOTO STRSAVELOOP
	
	C,
;
